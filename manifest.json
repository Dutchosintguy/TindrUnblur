{
  "manifest_version": 2,
  "author": "Radu Ursache",
  "name": "Tinder Free - See who likes you",
  "version": "1.0.0",
  "description": "Disable blur overlay in Tinder 'Who likes you' web page. Makes sense only for free accounts",
  "permissions": [
  	"tabs",
  	"http://tinder.com/app/likes-you",
    "https://tinder.com/app/likes-you"
  ],
  "background": {
    "scripts": ["content.js"],
    "persistent": false
  },
  "icons": { 
    "256": "tindr256.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}